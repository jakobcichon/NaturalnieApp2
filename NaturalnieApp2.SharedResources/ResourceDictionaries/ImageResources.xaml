<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    >

    <!--
    Filter/sorting images-->
    <BitmapImage x:Key="ClearFilterImage" UriSource="pack://application:,,,/NaturalnieApp2.SharedResources;Component/Images/clear-filter.png"/>
    <BitmapImage x:Key="SelectImage" UriSource="pack://application:,,,/NaturalnieApp2.SharedResources;Component/Images/select.png"/>
    <!--Idnicator images-->
