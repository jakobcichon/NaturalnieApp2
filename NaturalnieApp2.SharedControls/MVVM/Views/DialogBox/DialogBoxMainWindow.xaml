<Window x:Class="NaturalnieApp2.SharedControls.MVVM.Views.DialogBox.DialogBoxMainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NaturalnieApp2.SharedControls.MVVM.Views.DialogBox"
        xmlns:vm="clr-namespace:NaturalnieApp2.SharedControls.MVVM.ViewModels.DialogBox"
        mc:Ignorable="d"
        Title="MessageBoxMainWindow" Height="200" Width="300">
    <Window.Resources>
        <DataTemplate DataType="{x:Type vm:DialogBoxTwoButtonsViewModel}">
            <local:DialogBoxTwoButtonsView/>
        </DataTemplate>
    </Window.Resources>
    
    <Grid>
        <ContentPresenter Content="{Binding RelativeSource={RelativeSource AncestorType=Window}, Path=DataContext}"/>
    </Grid>
</Window>
